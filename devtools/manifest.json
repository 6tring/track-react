{
  "name": "ReactEvents",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Custom DevTools panel using React and Vite",
  "devtools_page": "./devtools.html",
  "permissions": [],
  "content_scripts": {
    "matches": ["<all_urls>"],
    "js": ["devtools/content.js"]
  },
  "action": {
    "default_title": "ReactEvents DevTools"
  }
}
